{
  "name": "npm-two-stage",
  "version": "10.0.0",
  "description": "Divide `npm install` into download and offline-install stages",
  "author": "Matthew Rafferty <mmraff@gmail.com>",
  "license": "Artistic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmraff/npm-two-stage.git"
  },
  "homepage": "https://github.com/mmraff/npm-two-stage/tree/npm10.1.0#readme",
  "bugs": {
    "url": "https://github.com/mmraff/npm-two-stage/issues"
  },
  "keywords": [
    "npm",
    "download",
    "install",
    "offline",
    "package",
    "air-gap",
    "sneakernet"
  ],
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "files": [
    "src/",
    "docs/"
  ],
  "scripts": {
    "test": "tap test/*_test.js",
    "integration-test": "tap --no-coverage --no-coverage-report test/integration.js"
  },
  "tap": {
    "after": "test/fixtures/arborist/fixtures/cleanup.js",
    "timeout": 800,
    "coverage-map": "test/config/index.js",
    "nyc-arg": [
      "--exclude=test/**",
      "--exclude=!test/tempAssets1/npm/lib/download/item-agents.js",
      "--exclude=!test/tempAssets2/npm/lib/download/alt-git.js",
      "--exclude=!test/tempAssets3/npm/lib/offliner/alt-arborist.js",
      "--exclude=!test/tempAssets4/npm/lib/offliner/build-ideal-tree.js",
      "--exclude=!test/tempAssets5/npm/lib/offliner/reify.js",
      "--exclude=!test/tempAssets6/npm/lib/commands/install.js",
      "--exclude=!test/tempAssets7/npm/lib/commands/download.js",
      "--exclude=!test/tempAssets8/npm/lib/npm.js",
      "--exclude=!test/staging/lib/node_modules/npm/lib/commands/download.js",
      "--exclude=!test/staging/lib/node_modules/npm/lib/download/*.js",
      "--exclude=!test/staging/lib/node_modules/npm/lib/commands/install.js",
      "--exclude=!test/staging/lib/node_modules/npm/lib/offliner/*.js"
    ]
  },
  "devDependencies": {
    "@isaacs/string-locale-compare": "^1.1.0",
    "@npmcli/arborist": "7.1.0",
    "@npmcli/fs": "^3.1.0",
    "@npmcli/run-script": "^7.0.1",
    "cmd-shim": "^6.0.0",
    "minify-registry-metadata": "^3.0.0",
    "nock": "^13.3.3",
    "npm": "10.1.0",
    "npm-package-arg": "^11.0.0",
    "pacote": "^17.0.4",
    "semver": "^7.5.4",
    "tap": "^16.3.8",
    "tar": "^6.1.15"
  }
}
